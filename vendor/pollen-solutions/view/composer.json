{
  "name": "pollen-solutions/view",
  "description": "Pollen Solutions - View Component - View template engine system.",
  "homepage": "https://www.presstify.com/pollen-solutions/view/",
  "keywords": [
    "pollen-solutions",
    "component",
    "view",
    "template engine"
  ],
  "time": "2021-07-09",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "league/plates": "^3.4",
    "pollen-solutions/faker": "^1.0",
    "pollen-solutions/support": "^1.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\View\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\View\\Test\\": "tests"
    }
  },
  "suggest": {
    "odan/plates-asset-cache" : "Caching and compression for Plates template assets (JavaScript and CSS).",
    "pollen-solutions/container" : "Pollen Solutions - Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
