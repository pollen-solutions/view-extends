{
  "name": "pollen-solutions/validation",
  "description": "Pollen Validation Component - Validation values tools.",
  "homepage": "https://www.presstify.com/pollen-solutions/validation/",
  "keywords": [
    "pollen-solutions",
    "component",
    "validation"
  ],
  "time": "2021-07-06",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "egulias/email-validator": "^3.0",
    "respect/validation": "^2.1",
    "symfony/validator": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Validation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Validation\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
